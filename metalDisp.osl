shader metalDisp (
output	float resultF=0,
output float disp=0
)
{
	point shaderP = transform("object",P);
	
	
	if (resultF == color ("rgb",0,0,0))
		disp = -0.2;


	//resultF for displacement
	resultF = 1-(float)texture("metalDisp1.tx",shaderP[0]/10.6+0.5,shaderP[1]/10.6+0.5);
	//resultF = 1-(float)texture("metalDisp.tx",shaderP[0]/6+0.5,shaderP[1]/6+0.5);
	resultF *= disp;


	//Cout=(color)texture("metal.tx",shaderP[0]/6+0.5,shaderP[1]/6+0.5);
}
